            android:id="@+id/home_radioGroup"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_50">
            <RadioButton
                android:textColor="@color/ff5181fb"
                android:textSize="@dimen/sp_15"
                android:id="@+id/home_radiobutton_inside_city"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_50"
                android:layout_weight="1"
                android:background="@drawable/myorder_choosed_color"
                android:button="@null"
                android:gravity="center"
                android:textStyle="bold"
                android:text="城内出行"/>

            <RadioButton
                android:id="@+id/home_radiobutton_intercity"
                android:textColor="@color/black"
                android:textSize="@dimen/sp_15"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_50"
                android:layout_weight="1"
                android:background="@drawable/myorder_nochoosed_color"
                android:button="@null"
                android:gravity="center"
                android:textStyle="bold"
                android:text="城际出行"/>
            <RadioButton
                android:id="@+id/home_radiobutton_transfer"
                android:textColor="@color/black"
                android:textSize="@dimen/sp_15"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_50"
                android:layout_weight="1"
                android:background="@drawable/myorder_nochoosed_color"
                android:button="@null"
                android:gravity="center"
                android:textStyle="bold"
                android:text="接送机"/>

        </RadioGroup>
    </RelativeLayout>

        <!--下单页面-->
